{
	"name": "input-material-ui",
	"version": "1.2.0",
	"description": "A material-ui INPUT component which allows to see/hide the password text of the field and to clear the input value",
	"main": "lib/index.js",
	"sideEffects": false,
	"scripts": {
		"build": "rimraf ./lib && tsc",
		"deploy": "cross-env NODE_ENV=production npm run build && npm publish",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/iulian-radu-at/input-material-ui.git"
	},
	"files": [
		"lib",
		"index.d.ts"
	],
	"keywords": [
		"text",
		"input",
		"password",
		"material-ui",
		"input-material-ui",
		"react",
		"react-component",
		"ui",
		"material design"
	],
	"author": "Iulian Radu <iulian.radu@gmx.at>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/iulian-radu-at/input-material-ui/issues"
	},
	"homepage": "https://github.com/iulian-radu-at/input-material-ui#readme",
	"dependencies": {
		"@material-ui/core": "^3.9.2",
		"@material-ui/icons": "^3.0.2",
		"lodash": "^4.17.11",
		"react": "^16.8.1",
		"react-dom": "^16.8.1"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@types/babel-core": "^6.25.5",
		"@types/enzyme": "^3.1.17",
		"@types/enzyme-adapter-react-16": "^1.0.3",
		"@types/lodash": "^4.14.120",
		"@types/react": "^16.8.2",
		"@types/react-dom": "^16.8.0",
		"@types/rimraf": "^2.0.2",
		"babel-core": "^6.26.3",
		"babel-jest": "^24.1.0",
		"cross-env": "^5.2.0",
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.9.1",
		"rimraf": "^2.6.3",
		"typescript": "^3.3.3"
	},
	"peerDependencies": {
		"react": "^16.0.0",
		"react-dom": "^16.0.0"
	},
	"jest": {
		"modulePathIgnorePatterns": [
			"./node_modules",
			"./history"
		],
		"transform": {
			"^.+\\.js$": "babel-jest"
		},
		"testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(js?))$",
		"setupFiles": [
			"./enzymeAdapter.setup.js"
		]
	}
}
